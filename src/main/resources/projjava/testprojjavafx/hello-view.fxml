<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projjava.testprojjavafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <ProgressBar fx:id="bar" prefWidth="200.0" progress="0.0" />

    <Label fx:id="welcomeText" />
   <Text fx:id="done" strokeType="OUTSIDE" strokeWidth="0.0" text="Done" />
    <Button fx:id="Hello" text="Hello!" />
</VBox>
